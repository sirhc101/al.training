/// <summary>
/// This enumeration is used to specify the supported color themes for inventory signal.
/// </summary>
enum 50001 Signal
{
    Extensible = true;

    value(0; Gray)
    {
        Caption = 'Gray';
    }
    value(1; Red)
    {
        Caption = 'Red';
    }
    value(2; Yellow)
    {
        Caption = 'Yellow';
    }
    value(3; Green)
    {
        Caption = 'Green';
    }
}